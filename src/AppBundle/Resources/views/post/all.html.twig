{% extends 'AppBundle::base.html.twig' %}


{% block body %}
    {% for key, post in posts %}
        <div class="blog-post">
            <h2 class="blog-post-title">{{ key+1 }}. {{ post.title }}</h2>
            <p class="blog-post-meta">{{ post.postDate }}</p>
            <p>{{ post.text }}</p>
        </div>
    {% endfor %}

{% endblock %}


